URL=

AUTH_URL=
AUTH_SECRET=

AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=

AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=

AUTH_FACEBOOK_ID=
AUTH_FACEBOOK_SECRET=

# On localhost only: Mock authentication simplifies testing mobile and multiple users
AUTH_MOCK_NAME=
AUTH_MOCK_EMAIL=

AWS_REGION=

S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_BUCKET_NAME=

# AWS SES requires owning a domain of the from address.
SES_FROM_EMAIL=
SES_ACCESS_KEY_ID=
SES_SECRET_ACCESS_KEY=

# Session Replay
NEXT_PUBLIC_REPLAY_ENABLED=false
# Optional: Scroll recording threshold as normalized ratio (0-1), e.g., 0.1 = record when scroll changes by 10% of scrollable area
# If omitted, scroll recording/replaying is disabled with zero performance overhead
# NEXT_PUBLIC_REPLAY_SCROLL_THRESHOLD_NORMALIZED=0.1

# If empty, all channels are logged. Example: "SCROLL,UI". To disable all: "__NONE__"
NEXT_PUBLIC_LOG_CHANNELS=
# If log level not defined, then read from server run parameter.
NEXT_PUBLIC_LOG_LEVEL=

# Comma-separated hostnames allowed for dev cross-origin requests (e.g. 10.0.0.41). Omit for none.
NEXT_ALLOWED_DEV_ORIGINS=

# Sentry (optional) error monitoring. DSN is public; used by client and server to send events. Do not commit SENTRY_AUTH_TOKEN.
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_AUTH_TOKEN=

# Posthog (optional) user engagement analytics.
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=
